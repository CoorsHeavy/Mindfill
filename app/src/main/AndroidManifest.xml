<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hudson.mindfill">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Quiz" />
        <activity android:name=".Dictionary" />
        <activity android:name=".DragDrop" />
        <activity android:name=".Schedule">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.INSERT_OR_EDIT" />

                <category android:name="android.intent.category.APP_CALENDAR" />

                <data android:mimeType="vnd.android.cursor.item/event" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Tiles"
            android:label="Improve Your Mood"
            android:theme="@style/AppTheme" />
        <activity android:name=".Chart"></activity>
    </application>

</manifest>